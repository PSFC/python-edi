		                "data_type_ids": null,
		                "length": {
		                    "min": 1,
		                    "max": 12
		                },
		                "notes": "A unique number assigned by the sender to identify a particular data segment in a hierarchical structure"
		            },
		            {
		                "id": "HL02",
		                "type": "element",
		                "name": "Hierarchical Parent ID Number",
		                "req": "O",
		                "data_type": "AN",
		                "data_type_ids": null,
		                "length": {
		                    "min": 1,
		                    "max": 12
		                },
		                "notes": "Identification number of the next higher hierarchical data segment that the data segment being described is subordinate to"
		            },
		            {
		                "id": "HL03",
		                "type": "element",
		                "name": "Hierarchical Level Code",
		                "req": "M",
		                "data_type": "ID",
		                "data_type_ids": "ID735",
		                "length": {
		                    "min": 1,
		                    "max": 2
		                },
		                "notes": "Code specifying the characteristic of a level in a hierarchical structure"
		            },
		            {
		                "id": "HL04",
		                "type": "element",
		                "name": "Hierarchical Child Code",
		                "req": "O",
		                "data_type": "ID",
		                "data_type_ids": "ID736",
		                "length": {
		                    "min": 1,
		                    "max": 1
		                },
		                "notes": "Code indicating if there are hierarchical child data segments subordinate to the level being described"
