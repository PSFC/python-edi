    {
        "id": "BSN",
        "type": "segment",
        "name": "Beginning Segment for Ship Notice",
        "req": "M",
        "max_uses": 1,
        "notes": "To transmit identifying numbers, dates, and other basic data relating to the transaction set",
		"syntax": [
            {"rule": "IFATLEASTONE", "criteria": [7, 6]}
        ],
        "elements": [
            {
                "id": "BSN01",
                "type": "element",
                "name": "Transaction Set Purpose Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID353",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code identifying purpose of transaction set"
            },
            {
                "id": "BSN02",
                "type": "element",
                "name": "Shipment Identification",
                "req": "M",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 2,
                    "max": 30
                },
                "notes": "A unique control number assigned by the original shipper to identify a specific shipment"
            },
            {
                "id": "BSN03",
                "type": "element",
                "name": "Date",
                "req": "M",
                "data_type": "DT",
                "data_type_ids": null,
                "length": {
                    "min": 8,
                    "max": 8
                },
                "notes": "Date expressed as CCYYMMDD (BSN03 is the date the shipment transaction set is created.)"
            },
            {
                "id": "BSN04",
                "type": "element",
                "name": "Time",
                "req": "M",
                "data_type": "TM",
                "data_type_ids": null,
                "length": {
                    "min": 4,
                    "max": 8
                },
                "notes": "Time expressed in 24-hour clock time as follows: HHMM, or HHMMSS, or HHMMSSD, or HHMMSSDD, where H = hours (00-23), M = minutes (00-59), S = integer seconds (00-59) and DD = decimal seconds; decimal seconds are expressed as follows: D = tenths (0-9) and DD = hundredths (00-99) (BSN04 is the time the shipment transaction set is created.)"
            },
            {
                "id": "BSN05",
                "type": "element",
                "name": "Hierarchical Structure Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID1005",
                "length": {
                    "min": 4,
                    "max": 4
                },
                "notes": "Code indicating the hierarchical application structure of a transaction set that utilizes the HL segment to define the structure of the transaction set"
            },
            {
                "id": "BSN06",
                "type": "element",
                "name": "Transaction Type Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID640",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code specifying the type of transaction"
            },
            {
                "id": "BSN07",
                "type": "element",
                "name": "Status Reason Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID641",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code indicating the status reason"
            }
        ]
    },
    {
