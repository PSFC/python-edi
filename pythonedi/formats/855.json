    {
        "id": "BAK",
        "type": "segment",
        "name": "Beginning Segment for Purchase Order Acknowledgment",
        "req": "M",
        "max_uses": 1,
        "notes": "",
        "elements": [
            {
                "id": "BAK01",
                "type": "element",
                "name": "Transaction Set Purpose Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID353",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code identifying purpose of transaction set"
            },
            {
                "id": "BAK02",
                "type": "element",
                "name": "Acknowledgment Type",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID587",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code specifying the type of acknowledgment"
            },
            {
                "id": "BAK03",
                "type": "element",
                "name": "Purchase Order Number",
                "req": "M",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 1,
                    "max": 22
                },
                "notes": "Identifying number for Purchase Order assigned by the orderer/purchaser"
            },
            {
                "id": "BAK04",
                "type": "element",
                "name": "Date",
                "req": "M",
                "data_type": "DT",
                "data_type_ids": null,
                "length": {
                    "min": 8,
                    "max": 8
                },
                "notes": "(BAK04 is the date assigned by the purchaser to purchase order.) Date expressed as CCYYMMDD where CC represents the first two digits of the calendar year"
            },
            {
                "id": "BAK05",
                "type": "element",
                "name": "Release Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 1,
                    "max": 30
                },
                "notes": "Number identifying a release against a Purchase Order previously placed by the parties involved in the transaction"
            },
            {
                "id": "BAK06",
                "type": "element",
                "name": "Request Reference Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 1,
                    "max": 45
                },
                "notes": "UNFI defined contract number"
            },
            {
                "id": "BAK07",
                "type": "element",
                "name": "Contract Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 1,
                    "max": 30
                },
                "notes": "Contract number"
            },
            {
                "id": "BAK08",
                "type": "element",
                "name": "Reference Identification",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 1,
                    "max": 80
                },
                "notes": "(Seller's Order Number) Reference information as defined for a particular Transaction Set or as specified by the Reference Identification Qualifier"
            },
            {
                "id": "BAK09",
                "type": "element",
                "name": "Date",
                "req": "M",
                "data_type": "DT",
                "data_type_ids": null,
                "length": {
                    "min": 8,
                    "max": 8
                },
                "notes": "(BAK09 is the date assigned by the sender to the acknowledgment.) Date expressed as CCYYMMDD where CC represents the first two digits of the calendar year"
            },
            {
                "id": "BAK10",
                "type": "element",
                "name": "Transaction Type Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID640",
                "length": {
                    "min": 2,
                    "max": 2
                },
                "notes": "Code specifying the type of transaction"
            }
        ]
