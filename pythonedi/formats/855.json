            },
            {
                "id": "TD503",
                "type": "element",
                "name": "Identification Code",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {
                    "min": 2,
                    "max": 80
                },
                "notes": "Code identifying a party or other code"
            },
            {
                "id": "TD504",
                "type": "element",
                "name": "Transportation Method/Type Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID91",
                "length": {
                    "min": 1,
                    "max": 2
                },
                "notes": "Code specifying the method or type of transportation for the shipment"
            }
        ]
    },
