        "req": "O",
        "max_uses": 12,
        "notes": "To specify the carrier and sequence of routing and provide transit time information",
		"syntax": [
            {"rule": "ATLEASTONE", "criteria": [2, 4, 5, 6, 12]},
            {"rule": "IFATLEASTONE", "criteria": [2, 3]},
            {"rule": "IFATLEASTONE", "criteria": [7, 8]},
            {"rule": "IFATLEASTONE", "criteria": [10, 11]},
            {"rule": "IFATLEASTONE", "criteria": [13, 12]},
            {"rule": "IFATLEASTONE", "criteria": [14, 13]},
            {"rule": "IFATLEASTONE", "criteria": [15, 12]}
        ],
        "elements": [
            {
                "id": "TD501",
                "type": "element",
                "name": "Routing Sequence Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID133",
                "length": {
                    "min": 1,
                    "max": 2
