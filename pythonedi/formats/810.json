                "notes": "To specify summary details of total items shipped in terms of quantity, weight, and volume",
                "syntax": [
                    {"rule": "ATLEASTONE", "criteria": [1, 3, 4]},
                    {"rule": "ALLORNONE", "criteria": [1, 2]},
                    {"rule": "ALLORNONE", "criteria": [3, 4]}
                ],
                "elements": [
                    {   "id": "ISS01", "type": "element",
                        "name": "Number of Units Shipped",
                        "req": "M",
                        "data_type": "R",
                        "data_type_ids": null,
                        "length": {"min": 1, "max": 10},
                        "notes": "Numeric value of units shipped in manufacturer's shipping units for a line item or transaction set"
                    },
                    {   "id": "ISS02", "type": "element",
                        "name": "Unit or Basis for Measurement Code",
                        "req": "M",
                        "data_type": "ID",
		                "data_type_ids": "ID355",
