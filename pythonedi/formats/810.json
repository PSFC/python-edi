    {   "id": "TDS", "type": "segment",
        "name": "Total Monetary Value Summary",
        "req": "M",
        "max_uses": 1,
        "notes": "To specify the total invoice discounts and amounts.",
        "elements": [
            {   "id": "TDS01", "type": "element",
                "name": "Total Invoice Amount",
                "req": "M",
                "data_type": "N2",
                "data_type_ids": null,
                "length": {"min": 1, "max": 15},
                "notes": "Amount before terms discount"
            },
            {   "id": "TDS02", "type": "element",
                "name": "Amt. Subj. to Terms Discount",
                "req": "O",
                "data_type": "N2",
                "data_type_ids": null,
                "length": {"min": 1, "max": 15},
                "notes": "Amount upon which the terms discount amount is calculated"
            },
            {   "id": "TDS03", "type": "element",
                "name": "Discounted Amount Due",
                "req": "O",
                "data_type": "N2",
                "data_type_ids": null,
                "length": {"min": 1, "max": 15},
                "notes": "Amount of invoice due if paid by terms discount due date"
            },
            {   "id": "TDS04", "type": "element",
                "name": "Terms Discount Amount",
                "req": "O",
                "data_type": "N2",
                "data_type_ids": null,
                "length": {"min": 1, "max": 15},
                "notes": "Total amount of terms discount"
            }
        ]
    },
    {   "id": "CAD", "type": "segment",
        "name": "Carrier Detail",
        "req": "O",
        "max_uses": 1,
        "notes": "To specify transportation details for the transaction",
        "elements": [
            {   "id": "CAD01", "type": "element",
                "name": "Transportation Method/Type Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID91",
                "length": {"min": 1, "max": 2},
                "notes": "Amount before terms discount"
            },
            {   "id": "CAD02", "type": "element",
                "name": "Equipment Initial",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 4},
                "notes": "Prefix or alphabetic part of an equipment unit's identifying number"
            },
            {   "id": "CAD03", "type": "element",
                "name": "CAD03",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 1},
                "notes": "CAD03"
            },
            {   "id": "CAD04", "type": "element",
                "name": "CAD04",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 1},
                "notes": "CAD04"
            },
            {   "id": "CAD05", "type": "element",
                "name": "Routing",
                "req": "M",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 35},
                "notes": "Free-form description of the routing or requested routing for shipment, or the originating carrier's identity"
            },
            {   "id": "CAD06", "type": "element",
                "name": "Shipment/Order Status Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID368",
                "length": {"min": 2, "max": 2},
                "notes": "Code indicating the status of an order or shipment or the disposition of any difference between the quantity ordered and the quantity shipped for a line item or transaction"
            },
            {   "id": "CAD07", "type": "element",
                "name": "Reference Identification Qualifier",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID128",
                "length": {"min": 2, "max": 3},
                "notes": "Code qualifying the Reference Identification"
            },
            {   "id": "CAD08", "type": "element",
                "name": "Reference Identification",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 30},
                "notes": "Reference information as defined for a particular Transaction Set or as specified by the Reference Identification Qualifier"
            }
        ]
    },
    {   "id": "TXI", "type": "segment",
        "name": "Tax Information",
        "req": "O",
        "max_uses": 10,
        "notes": "To specify tax information",
        "syntax": [
            {"rule": "ATLEASTONE", "criteria": [2, 3, 6]},
            {"rule": "ALLORNONE", "criteria": [4, 5]},
            {"rule": "IFATLEASTONE", "criteria": [8, 3]}
        ],
        "elements": [
            {   "id": "TXI01", "type": "element",
                "name": "Tax Type Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID963",
                "length": {"min": 2, "max": 2},
                "notes": "Amount before terms discount"
            },
            {   "id": "TXI02", "type": "element",
                "name": "Monetary Amount",
                "req": "O",
                "data_type": "R",
                "data_type_ids": null,
                "length": {"min": 1, "max": 18},
                "notes": "Monetary amount"
            },
            {   "id": "TXI03", "type": "element",
                "name": "Percent",
                "req": "O",
                "data_type": "R",
                "data_type_ids": null,
                "length": {"min": 1, "max": 10},
                "notes": "Percentage expressed as a decimal"
            },
            {   "id": "TXI04", "type": "element",
                "name": "Tax Jurisdiction Code Qualifier",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": {"CD": "Customer Defined", "VD": "Vendor Defined", "VE": "Vertex"},
                "length": {"min": 2, "max": 2},
                "notes": "Code identifying the source of the data used in tax jurisdiction code"
            },
            {   "id": "TXI05", "type": "element",
                "name": "Tax Jurisdiction Code",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 10},
                "notes": "Code identifying the taxing jurisdiction"
            },
            {   "id": "TXI06", "type": "element",
                "name": "Tax Exempt Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID441",
                "length": {"min": 1, "max": 1},
                "notes": "Code identifying exemption status from sales and use tax"
            },
            {   "id": "TXI07", "type": "element",
                "name": "Relationship Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID662",
                "length": {"min": 1, "max": 1},
                "notes": "Code indicating the relationship between entities"
            },
            {   "id": "TXI08", "type": "element",
                "name": "Dollar Basis for Percent",
                "req": "O",
                "data_type": "R",
                "data_type_ids": null,
                "length": {"min": 1, "max": 9},
                "notes": "Dollar basis to be used in the percent calculation of the allowance, charge, or tax"
            },
            {   "id": "TXI09", "type": "element",
                "name": "Tax Identification Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 20},
                "notes": "Number assigned to a purchaser (buyer, orderer) by a taxing jurisdiction (state, county, etc.); often called a tax exemption number or certificat number"
            },
            {   "id": "TXI10", "type": "element",
                "name": "Assigned Identification",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 20},
                "notes": "Alphanumeric characters assigned for differentiation within a transaction set"
            }
        ]
    },
