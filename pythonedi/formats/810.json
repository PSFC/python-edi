        "max_uses": 1,
        "notes": "",
        "elements": [
            {   "id": "BIG01", "type": "element",
                "name": "Invoice Date",
                "req": "M",
                "data_type": "DT",
                "data_type_ids": null,
                "length": {"min": 8, "max": 8},
                "notes": "Invoice issue date"
            },
            {   "id": "BIG02", "type": "element",
                "name": "Invoice Number",
                "req": "M",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 22},
                "notes": "Identifying number assigned by supplier"
            },
            {   "id": "BIG03", "type": "element",
                "name": "Purchase Order Date",
                "req": "O",
                "data_type": "DT",
                "data_type_ids": null,
                "length": {"min": 8, "max": 8},
                "notes": "Original date (assigned by hospital) of the purchase order"
            },
            {   "id": "BIG04", "type": "element",
                "name": "Purchase Order Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 22},
                "notes": "Identifying number for purchase order assigned by hospital"
            },
            {   "id": "BIG05", "type": "element",
                "name": "Release Number",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 30},
                "notes": "Only used if hospital sent the release # in original PO (element BEG04)"
            },
            {   "id": "BIG06", "type": "element",
                "name": "Change Order Seq. #",
                "req": "O",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 8},
                "notes": "Not used"
            },
            {   "id": "BIG07", "type": "element",
                "name": "Transaction Type Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID640",
                "length": {"min": 1, "max": 22},
                "notes": "Identifying number for purchase order assigned by hospital"
            },
            {   "id": "BIG08", "type": "element",
                "name": "Transaction Set Purpose Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID353",
                "length": {"min": 2, "max": 2},
                "notes": "Not used"
            }
        ]
    },
    {   "id": "NTE", "type": "segment",
        "name": "Note/Special Instruction",
        "req": "O",
        "max_uses": 100,
        "notes": "The NTE segment permits free-form information/data which, under ANSI X12 standard implementations, is not machine processable. The use of the NTE segment should therefore be avoided, if at all possible, in an automated environment.",
        "elements": [
            {   "id": "NTE01", "type": "element",
                "name": "Note Reference Code",
                "req": "O",
                "data_type": "ID",
                "data_type_ids": "ID363",
                "length": {"min": 3, "max": 3},
                "notes": "Code identifying the functional area or purpose for which the note applies"
            },
            {   "id": "NTE02", "type": "element",
                "name": "Description",
                "req": "M",
                "data_type": "AN",
                "data_type_ids": null,
                "length": {"min": 1, "max": 80},
                "notes": "A free-form description to clarify the related data elements and their content"
            }
        ]
    },
    {   "id": "CUR", "type": "segment",
        "name": "Currency",
        "req": "O",
        "max_uses": 1,
        "notes": "If the CUR segment is not used, monetary values are assumed to be expressed in the currency of the country of the transaction originator.",
        "elements": [
            {   "id": "CUR01", "type": "element",
                "name": "Entity Identifier Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID98",
                "length": {"min": 2, "max": 3},
                "notes": "Code identifying an organizational entity, a physical location, property or an individual"
            },
            {   "id": "CUR02", "type": "element",
                "name": "Currency Code",
                "req": "M",
                "data_type": "ID",
                "data_type_ids": "ID100",
                "length": {"min": 3, "max": 3},
                "notes": "Code (Standard ISO) for country in whose currency the charges are specified"
            },
            {   "id": "CUR03", "type": "element",
                "name": "Exchange Rate",
                "req": "O",
                "data_type": "R",
                "data_type_ids": null,
                "length": {"min": 4, "max": 10},
                "notes": "Value to be used as a multiplier conversion factor to convert monetary value from one currency to another"
            }
        ]
    },
